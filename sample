PUT /ecommerce
{
  "mappings":{
    "product":{
      "properties": {
        "name":{
          "type": "text"
        },
        "price":{
          "type": "double"
        },
        "description":{
          "type": "text"
        },
        "status":{
          "type": "text"
        },
        "quantity":{
          "type": "integer"
        },
        "categories":{
          "type": "nested",
          "properties": {
            "name":{
              "type":"text"
            }
          }
        },
        "tags":{
          "type": "text"
        }
      }
    }
  }
}

