GET /ecommerce/product/_search
{
  "query": {
    "bool": {
      "must": [
        {"match": {
          "name": "pasta"
        }}
      ],
      "should": [
        {
          "match": {
            "name": {
              "query":"spaghetti",
              "boost":2.0
            }
          }
        },  
        {
          "match": {
            "name": {
              "query": "noodle",
              "boost":1.5
            }
          }
        }
      ]
    }
  }
}


GET ecommerce/product/_search
{
  "query": {
    "bool": {
      "must": [
        {
          "match": {
            "name": "pasta"
          }
        }
      ],
      "filter": [{
        "range":{
          "quantity":{
            "gte":10,
            "lte":15
          }
        }
      }]
    }
  }
}

