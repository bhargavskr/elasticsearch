GET /ecommerce/product/_search
{
  "query": {
    "bool": {
      "must": [
        {"match": {
          "name": "pasta"
        }},
        {
          "match": {
            "name": "spaghetti"
          }
        }
      ]
    }
  }
}

GET /ecommerce/product/_search
{
  "query": {
    "bool": {
      "must": [
        {"match": {
          "name": "pasta"
        }}
      ],
      "must_not": [
        {
          "match": {
            "name": "spaghetti"
          }
        }
      ]
    }
  }
}

GET /ecommerce/product/_search
{
  "query": {
    "bool": {
      "must": [
        {"match": {
          "name": "pasta"
        }}
      ],
      "should": [
        {
          "match": {
            "name": "spaghetti"
          }
        }
      ]
    }
  }
}


GET /ecommerce/product/_search
{
  "query": {
    "bool": {
      
      "should": [
        {
          "match": {
            "name": "spaghetti"
          }
        }
      ]
    }
  }
}

